{
  "name": "meeting-transcript-app",
  "version": "1.0.0",
  "description": "A minimal dockerized application that gets transcripts from meetings using Recall.ai API",
  "main": "dist/backend/server.js",
  "scripts": {
    "build": "tsc",
    "build:frontend": "cd src/frontend && npm run build",
    "start": "node dist/backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/backend/server.ts",
    "dev:frontend": "cd src/frontend && npm run dev",
    "test": "jest"
  },
  "keywords": [
    "meeting",
    "transcript",
    "recall.ai",
    "redis",
    "docker"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "openai": "^5.12.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
